  "license": "ISC",
  "devDependencies": {
    "d3": "^6.5.0",
    "three": "^0.125.1",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@seregpie/three.text-sprite": "^3.2.0",
    "firebase": "^8.2.5",
    "firebase-tools": "^9.2.2"
  }
