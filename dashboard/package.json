{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "config": {
    "build_rpc": "./remote_build_scripts/generate_rpc_interface.sh --build_machine \"$npm_config_build_machine\""
  },
  "scripts": {
    "watch": "$npm_package_config_build_rpc && npx webpack --watch",
    "start": "$npm_package_config_build_rpc && npx webpack serve",
    "build": "$npm_package_config_build_rpc && npx webpack",
    "preview": "$npm_package_config_build_rpc && npx webpack && npx firebase hosting:channel:deploy preview",
    "publish": "$npm_package_config_build_rpc && npx webpack && npx firebase deploy --only hosting"
  },
